    "@typescript-eslint/parser": "^5.14.0",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "microbundle": "^0.14.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
