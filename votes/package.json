{
  "name": "votes",
  "main": "dist/votes.cjs.js",
  "module": "dist/votes.esm.js",
  "version": "2.0.4",
  "description": "Implementation of some [electoral systems](https://en.wikipedia.org/wiki/Electoral_system)",
  "keywords": [
    "votes",
    "condorcet",
    "voting-systems",
    "elections"
  ],
  "sideEffects": false,
  "types": "dist/votes.cjs.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "author": "lzear",
  "repository": {
    "type": "git",
    "url": "https://github.com/lzear/votes.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test-prod": "yarn run test && yarn run build",
    "prebuild": "rimraf dist",
    "build": "preconstruct build && yarn typedoc",
    "publish-docs": "node tools/gh-pages-publish.mjs",
    "release": "cp ../README.md . && yarn run build && changeset publish",
    "typedoc": "npx typedoc src/index.ts --tsconfig tsconfig.typedoc.json"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.3",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "dotenv": "^16.0.1",
    "jest": "^28.1.2",
    "microbundle": "^0.15.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.23.6",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/lzear/votes/issues"
  },
  "homepage": "https://github.com/lzear/votes",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "volta": {
    "node": "16.16.0"
  }
}
