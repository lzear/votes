{
  "name": "votes",
  "version": "",
  "description": "Implementation of some [electoral systems](https://en.wikipedia.org/wiki/Electoral_system)",
  "keywords": [],
  "sideEffects": false,
  "types": "dist/votes.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "author": "lzear",
  "repository": {
    "type": "git",
    "url": "https://github.com/lzear/votes.git"
  },
  "license": "MIT",
  "source": "src/votes.ts",
  "exports": {
    "require": "./dist/votes.cjs",
    "default": "./dist/votes.modern.js"
  },
  "type": "module",
  "module": "dist/votes.js",
  "main": "dist/votes.cjs",
  "unpkg": "./dist/votes.umd.js",
  "scripts": {
    "eslint": "eslint --ext .js --ext .ts .",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "build": "microbundle",
    "build:watch": "microbundle watch",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "yarn run eslint && yarn run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "updatedeps": "npx npm-check-updates -u"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "commitizen": "^4.2.4",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.2",
    "microbundle": "^0.14.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1",
    "shelljs": "^0.8.4",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.10",
    "typescript": "4.5.3"
  },
  "bugs": {
    "url": "https://github.com/lzear/votes/issues"
  },
  "homepage": "https://github.com/lzear/votes",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "next",
        "channel": "next"
      }
    ]
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
